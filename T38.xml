<FDM_CONFIG NAME="T38" VERSION="1.61">
<!--
  File:     T38.xml
  Author:   Aero-Matic v 0.4, modified 15 Sep 2003, David Culp

  Inputs:
    name:          T38
    type:          two-engine transonic/supersonic fighter
    max weight:    12500.0 lb
    wing span:     25.25 ft
    length:        46.3 ft
    wing area:     170.0 sq-ft
    gear type:     tricycle
    retractable?:  yes
    # engines:     2
    engine type:   turbine
    engine layout: aft fuselage
    yaw damper?    yes
  Outputs:
    wing loading:  73.53 lb/sq-ft
    CL-alpha:      4 per radian   // changing this to 2.855
    CL-0:          0.08
    CL-max:        1.4
    CD-0:          0.025 // changed this to 0.016
    K:             0.05  // changing this to 0.10

-->

 <METRICS>
   AC_WINGAREA  170.00
   AC_WINGSPAN  25.25
   AC_CHORD      6.73
   AC_HTAILAREA 34.00
   AC_HTAILARM  13.00
   AC_VTAILAREA 47.00
   AC_LV        18.52
   AC_IXX       7806
   AC_IYY       36075
   AC_IZZ       31319
   AC_IXZ       0
   AC_EMPTYWT   7574
   AC_CGLOC     310.0  0.0 -10.0
   AC_AERORP    320.0  0.0  0.0
   AC_EYEPTLOC  154.0  0.0 36.0
   AC_VRP       0 0 0
 </METRICS>
 <UNDERCARRIAGE>
  AC_GEAR NOSE_LG   140.0   0.0 -60.0 5000   500 0.5 0.8 0.02 STEERABLE NONE  5  RETRACT
  AC_GEAR LEFT_MLG  335.0 -65.0 -60.0 37870 3787 0.5 0.8 0.02 FIXED     LEFT  0  RETRACT
  AC_GEAR RIGHT_MLG 335.0  65.0 -60.0 37870 3787 0.5 0.8 0.02 FIXED     RIGHT 0  RETRACT
 </UNDERCARRIAGE>
 <PROPULSION>
  <AC_ENGINE FILE="J85-GE-5_sim">
    XLOC 452.0
    YLOC -20
    ZLOC 0
    PITCH 0
    YAW 0
    FEED 0
  </AC_ENGINE>
  <AC_THRUSTER FILE="direct">
    XLOC 551.0
    YLOC -20
    ZLOC 0
    PITCH 0.03
    YAW 0
  </AC_THRUSTER>
  <AC_ENGINE FILE="J85-GE-5_sim">
    XLOC 452.0
    YLOC 20
    ZLOC 0
    PITCH 0
    YAW 0
    FEED 1
  </AC_ENGINE>
  <AC_THRUSTER FILE="direct">
    XLOC 551.0
    YLOC 20
    ZLOC 0
    PITCH 0.03
    YAW 0
  </AC_THRUSTER>
  <AC_TANK TYPE="FUEL" NUMBER="0"> <!-- left/fwd -->
    XLOC 298.0 
    YLOC 0 
    ZLOC 0 
    RADIUS 1 
    CAPACITY 1953 
    CONTENTS 1900 
  </AC_TANK>
  <AC_TANK TYPE="FUEL" NUMBER="1"> <!-- right/aft -->
    XLOC 370.0 
    YLOC 0 
    ZLOC 0 
    RADIUS 1 
    CAPACITY 2029
    CONTENTS 2000 
  </AC_TANK>
 </PROPULSION>
 <FLIGHT_CONTROL NAME="T38-FCS">
   <!-- PITCH FCS ********************************************************* -->
   <COMPONENT NAME="Pilot Pitch Sum" TYPE="SUMMER">
      INPUT   fcs/elevator-cmd-norm
      INPUT   fcs/pitch-trim-cmd-norm
      CLIPTO  -1 1
   </COMPONENT>
   <COMPONENT NAME="Pitch Feel" TYPE="SCHEDULED_GAIN">
      INPUT        fcs/pilot-pitch-sum
      SCHEDULED_BY velocities/mach-norm
      ROWS         2
       0.2         1.0
       1.4         0.2 
   </COMPONENT>
   <COMPONENT NAME="Elevator Control" TYPE="AEROSURFACE_SCALE">
      INPUT   fcs/pitch-feel
      MIN    -0.300
      MAX     0.175
      OUTPUT  fcs/elevator-pos-rad
   </COMPONENT>
   <!-- ROLL FCS ********************************************************** -->
   <COMPONENT NAME="Pilot Roll Sum" TYPE="SUMMER">
      INPUT   fcs/aileron-cmd-norm
      INPUT   fcs/roll-trim-cmd-norm
      CLIPTO  -1 1
   </COMPONENT>
   <COMPONENT NAME="Roll Feel" TYPE="SCHEDULED_GAIN">
      INPUT        fcs/pilot-roll-sum
      SCHEDULED_BY velocities/mach-norm
      ROWS         2
       0.4         1.0
       1.4         0.4 
   </COMPONENT>
   <COMPONENT NAME="Left Aileron Control" TYPE="AEROSURFACE_SCALE">
      INPUT   fcs/roll-feel
      MIN    -0.35
      MAX     0.26
      OUTPUT  fcs/left-aileron-pos-rad
   </COMPONENT>
   <COMPONENT NAME="Right Aileron Control" TYPE="AEROSURFACE_SCALE">
      INPUT   -fcs/roll-feel
      MIN    -0.35
      MAX     0.26
      OUTPUT  fcs/right-aileron-pos-rad
   </COMPONENT>
   <!-- YAW FCS *********************************************************** -->
   <COMPONENT NAME="Rudder Command Sum" TYPE="SUMMER">
      INPUT       fcs/rudder-cmd-norm
      INPUT       fcs/yaw-trim-cmd-norm
      CLIPTO      -1 1
   </COMPONENT>
   <COMPONENT NAME="Yaw SAS Rate" TYPE="SCHEDULED_GAIN">
      INPUT        velocities/r-aero-rad_sec
      SCHEDULED_BY velocities/ve-kts
      ROWS         2
       30          0.00
       60          3.00
   </COMPONENT>
   <COMPONENT NAME="Yaw SAS Beta" TYPE="SCHEDULED_GAIN">
      INPUT        aero/beta-rad
      SCHEDULED_BY velocities/ve-kts
      ROWS         2
       30          0.00
       60          0.00
   </COMPONENT>
   <COMPONENT NAME="Yaw SAS Sum" TYPE="SUMMER">
      INPUT        fcs/yaw-sas-rate
      INPUT        fcs/yaw-sas-beta
      CLIPTO       -0.1 0.1
   </COMPONENT>
   <COMPONENT NAME="Rudder Sum" TYPE="SUMMER">
      INPUT        fcs/rudder-command-sum
      INPUT        fcs/yaw-sas-sum
      CLIPTO       -1 1
   </COMPONENT>
   <COMPONENT NAME="Rudder Control" TYPE="AEROSURFACE_SCALE">
      INPUT        fcs/rudder-sum
      MIN         -0.35
      MAX          0.35
      OUTPUT       fcs/rudder-pos-rad
   </COMPONENT>
   <!-- FLAPS TRAVEL TIME ************************************************* -->
   <COMPONENT NAME="Flaps Control" TYPE="KINEMAT">
     INPUT   fcs/flap-cmd-norm
     DETENTS 3
             0    0
             24   5
             40   3
     OUTPUT  fcs/flap-pos-deg
   </COMPONENT>
   <!-- GEAR TRAVEL TIME ************************************************** -->
   <COMPONENT NAME="Gear Control" TYPE="KINEMAT">
     INPUT   gear/gear-cmd-norm
     DETENTS 2
             0   0
             1   3
     OUTPUT  gear/gear-pos-norm
   </COMPONENT>
   <!-- SPEEDBRAKE TRAVEL TIME ******************************************** -->
   <COMPONENT NAME="Speedbrake Control" TYPE="KINEMAT">
     INPUT   fcs/speedbrake-cmd-norm
     DETENTS 2
             0   0
             1   1
     OUTPUT  fcs/speedbrake-pos-norm
   </COMPONENT>
 </FLIGHT_CONTROL>
 <AERODYNAMICS>
  <AXIS NAME="LIFT">
    <COEFFICIENT NAME="CLalpha" TYPE="TABLE">
      Lift_due_to_alpha
      5
      2
      aero/alpha-rad
      fcs/components/flaps-control/output-norm
      aero/qbar-psf|metrics/Sw-sqft
                   0     1.0
      -0.20   -0.571     0.100
       0.00    0.000     0.600
       0.25    0.716     1.300
       0.33    0.945     0.300
       0.60    0.174     0.000 
    </COEFFICIENT>
    <COEFFICIENT NAME="CLde" TYPE="VALUE">
       Lift_due_to_Elevator_Deflection
       aero/qbar-psf|metrics/Sw-sqft|fcs/elevator-pos-rad
       0.2
    </COEFFICIENT>
  </AXIS>
  <AXIS NAME="DRAG">
    <COEFFICIENT NAME="CD0" TYPE="VECTOR">
       Drag_at_zero_lift
       5
       aero/alpha-rad
       aero/qbar-psf|metrics/Sw-sqft
       -1.57       1.500
       -0.26       0.032
        0.00       0.016
        0.26       0.032
        1.57       1.500
    </COEFFICIENT>
    <COEFFICIENT NAME="CDi" TYPE="VALUE">
       Induced_drag
       aero/qbar-psf|metrics/Sw-sqft|aero/cl-squared-norm
       0.1
    </COEFFICIENT>
    <COEFFICIENT NAME="CDmach" TYPE="VECTOR">
       Drag_due_to_mach
       4
       velocities/mach-norm
       aero/qbar-psf|metrics/Sw-sqft
       0.0       0.000
       0.81      0.000
       1.1       0.024
       1.8       0.011
    </COEFFICIENT>
    <COEFFICIENT NAME="CDflap" TYPE="VALUE">
       Drag_due_to_flaps
       aero/qbar-psf|metrics/Sw-sqft|fcs/components/flaps-control/output-norm
       0.09
    </COEFFICIENT>
    <COEFFICIENT NAME="CDgear" TYPE="VALUE">
       Drag_due_to_gear
       aero/qbar-psf|metrics/Sw-sqft|gear/gear-pos-norm
       0.015
    </COEFFICIENT>
    <COEFFICIENT NAME="CDsb" TYPE="VALUE">
       Drag_due_to_speedbrakes
       aero/qbar-psf|metrics/Sw-sqft|fcs/speedbrake-pos-norm
       0.025
    </COEFFICIENT>
    <COEFFICIENT NAME="CDbeta" TYPE="VECTOR">
       Drag_due_to_sideslip
       5
       aero/beta-rad
       aero/qbar-psf|metrics/Sw-sqft
       -1.57     1.23
       -0.26     0.05
        0.00     0.00  
        0.26     0.05
        1.57     1.23       
    </COEFFICIENT>
    <COEFFICIENT NAME="CDde" TYPE="VALUE">
       Drag_due_to_Elevator_Deflection
       aero/qbar-psf|metrics/Sw-sqft|fcs/elevator-pos-norm
       0.03
    </COEFFICIENT>
  </AXIS>
  <AXIS NAME="SIDE">
    <COEFFICIENT NAME="CYb" TYPE="VALUE">
       Side_force_due_to_beta
       aero/qbar-psf|metrics/Sw-sqft|aero/beta-rad
       -1
    </COEFFICIENT>
  </AXIS>
  <AXIS NAME="ROLL">
    <COEFFICIENT NAME="Clb" TYPE="VECTOR">
       Roll_moment_due_to_beta
       2
       aero/alpha-rad
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|aero/beta-rad
       0.00    -0.05
       0.52    -0.12
    </COEFFICIENT>
    <COEFFICIENT NAME="Clp" TYPE="VALUE">
       Roll_moment_due_to_roll_rate
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|aero/bi2vel|velocities/p-aero-rad_sec
       -0.4
    </COEFFICIENT>
    <COEFFICIENT NAME="Clr" TYPE="VALUE">
       Roll_moment_due_to_yaw_rate
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|aero/bi2vel|velocities/r-aero-rad_sec
       0.12
    </COEFFICIENT>
    <COEFFICIENT NAME="CldaL" TYPE="VALUE">
       Roll_moment_due_to_left_aileron
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|fcs/left-aileron-pos-rad
       0.055
    </COEFFICIENT>
    <COEFFICIENT NAME="CldaR" TYPE="VALUE">
       Roll_moment_due_to_right_aileron
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|fcs/right-aileron-pos-rad
       -0.055
    </COEFFICIENT>
    <COEFFICIENT NAME="Cldr" TYPE="VALUE">
       Roll_moment_due_to_rudder
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|fcs/rudder-pos-rad
       0.006
    </COEFFICIENT>
  </AXIS>
  <AXIS NAME="PITCH">
    <COEFFICIENT NAME="Cmalpha" TYPE="VALUE">
       Pitch_moment_due_to_alpha
       aero/qbar-psf|metrics/Sw-sqft|metrics/cbarw-ft|aero/alpha-rad
       -0.3
    </COEFFICIENT>
    <COEFFICIENT NAME="Cmde" TYPE="VALUE">
       Pitch_moment_due_to_elevator
       aero/qbar-psf|metrics/Sw-sqft|metrics/cbarw-ft|fcs/elevator-pos-rad
       -0.65
    </COEFFICIENT>
    <COEFFICIENT NAME="Cmq" TYPE="VALUE">
       Pitch_moment_due_to_pitch_rate
       aero/qbar-psf|metrics/Sw-sqft|metrics/cbarw-ft|aero/ci2vel|velocities/q-aero-rad_sec
       -18
    </COEFFICIENT>
    <COEFFICIENT NAME="Cmadot" TYPE="VALUE">
       Pitch_moment_due_to_alpha_rate
       aero/qbar-psf|metrics/Sw-sqft|metrics/cbarw-ft|aero/ci2vel|aero/alphadot-rad_sec
       -9
    </COEFFICIENT>
    <COEFFICIENT NAME="Cmflaps" TYPE="VALUE">
       Pitch_moment_due_to_flaps
       aero/qbar-psf|metrics/Sw-sqft|metrics/cbarw-ft|fcs/components/flaps-control/output-norm
       -0.1
    </COEFFICIENT>
  </AXIS>
  <AXIS NAME="YAW">
    <COEFFICIENT NAME="Cnb" TYPE="VALUE">
       Yaw_moment_due_to_beta
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|aero/beta-rad
       0.1
    </COEFFICIENT>
    <COEFFICIENT NAME="Cnr" TYPE="VALUE">
       Yaw_moment_due_to_yaw_rate
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|aero/bi2vel|velocities/r-aero-rad_sec
       -0.15
    </COEFFICIENT>
    <COEFFICIENT NAME="Cndr" TYPE="VALUE">
       Yaw_moment_due_to_rudder
       aero/qbar-psf|metrics/Sw-sqft|metrics/bw-ft|fcs/rudder-pos-rad
       -0.02
    </COEFFICIENT>
  </AXIS>
 </AERODYNAMICS>
</FDM_CONFIG>
